<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.controllers.EditRoomController">
    <VBox alignment="TOP_CENTER" layoutX="82.0" layoutY="14.0" prefHeight="200.0" prefWidth="136.0" spacing="15.0">
        <Label text="Edit">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="24.0"/>
            </font>
        </Label>
        <TextField fx:id="roomID" disable="true" promptText="Room ID"/>
        <TextField fx:id="owner" promptText="Owner"/>
        <TextField fx:id="price" promptText="Price"/>
        <TextField fx:id="status" disable="true" promptText="Status"/>
        <TextField fx:id="reservedOn" disable="true" promptText="Reserved on"/>
        <Button mnemonicParsing="false" prefWidth="70.0" text="Apply" onAction="#apply"/>
    </VBox>
</AnchorPane>
